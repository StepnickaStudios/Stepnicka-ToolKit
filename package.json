{
  "name": "stepnicka-toolkit",
  "displayName": "Stepnicka ToolKit",
  "description": "Electron, HTML, CSS, API, TailwindCSS, And Elements For HTML, ToolKit For Developer Junior/FullStack",
  "author": "StepnickaStudios",
  "version": "1.1.2",
  "activationEvents": [
    "onCommand:stepnicka.insertSnippet",
    "onCommand:stepnicka.apoyar"
  ],
  "keywords": ["electron", "snippets", "electronjs", "html", "css", "tailwindcss", "api", "javascript", "adm-zip"],
  "icon": "Icon.png",
  "license": "MIT",
  "publisher": "StepnickaStudio",
  "main": "./src/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StepnickaStudios/Stepnicka-ToolKit.git"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Snippets",
    "Visualization",
    "Debuggers",
    "Language Packs"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html.code-snippets"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "stepnickaSnippetsContainer",
          "title": "Snippets ST",
          "icon": "Stepnicka-ToolKit.png"
        }
      ]
    },
    "views": {
      "stepnickaSnippetsContainer": [
        {
          "id": "stepnickaSnippets",
          "name": "Lista De Snippets",
          "icon": "Stepnicka-ToolKit.png"
        }
      ]
    },
    "commands": [
      {
        "command": "stepnicka.insertSnippet",
        "title": "Insertar Snippet"
      },
      {
        "command": "stepnicka.apoyar",
        "title": "❤️ Apoyar Proyecto"
      }
    ]
  }
}
